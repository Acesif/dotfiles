#keystate_numlock = enable
#keystate_scrolllock = enable
#keystate_capslock = enable



#mute
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
    m:0x10 + c:121
    Mod2 + XF86AudioMute 

#down
"pactl set-sink-volume @DEFAULT_SINK@ -1%"
    m:0x10 + c:122
    Mod2 + XF86AudioLowerVolume 

#up
"pactl set-sink-volume @DEFAULT_SINK@ +1%"
    m:0x10 + c:123
    Mod2 + XF86AudioRaiseVolume 

#play/pause
"playerctl play-pause"
    m:0x10 + c:172
    Mod2 + XF86AudioPlay 

#next
"playerctl next"
    m:0x10 + c:171
    Mod2 + XF86AudioNext 

#prev
"playerctl previous"
    m:0x10 + c:173
    Mod2 + XF86AudioPrev 

#mutevol
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
    m:0x10 + c:75
    Mod2 + F9 

#ss
"flameshot gui"
    m:0x10 + c:118
    Mod2 + Insert 

#clipboard
"clipmenu"
    m:0x18 + c:52
    Alt+Mod2 + z 

#
# End of xbindkeys configuration
